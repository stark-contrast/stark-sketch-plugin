{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "images/icon.png",
  "commands": [
    {
      "name": "Vision Simulator",
      "identifier": "simulator",
      "shortcut": "cmd shift u",
      "script": "vision-simulator.js"
    },
    {
      "name": "Check Contrast",
      "identifier": "contrast",
      "shortcut": "cmd shift p",
      "script": "contrast.js",
      "handler": "checkContrast"
    },
    {
      "name": "Focus Order",
      "identifier": "focus",
      "shortcut": "cmd shift i",
      "script": "focus-order.js",
      "handler": "focusOrder"
    },
    {
      "name": "Registration",
      "identifier": "registration",
      "script": "registration.js"
    },
    {
      "name": "Get Help with Stark",
      "identifier": "gethelp",
      "script": "gethelp.js"
    },
    {
      "name": "Selection Changed",
      "identifier": "selection-changed",
      "script": "contrast.js",
      "handlers": {
        "actions": {
          "SelectionChanged.finish": "onSelectionChanged"
        }
      }
    }
  ],
  "menu": {
    "title": "Stark",
    "items": [
      "simulator",
      "contrast",
      "focus",
      "-",
      "registration",
      "-",
      "gethelp"
    ]
  },
  "identifier": "com.mfouquet.sketch.stark",
  "version": "2.37",
  "description": "Ensure your design is accessible and high contrast for every type of color blindness.",
  "author": "Stark Lab, Inc.",
  "authorEmail": "michael@getstark.co",
  "appcast": "https://raw.githubusercontent.com/stark-contrast/stark-sketch-plugin/main/appcast.xml",
  "name": "Stark",
  "disableCocoaScriptPreprocessor": true
}